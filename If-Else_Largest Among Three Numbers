#include <stdio.h>

int main() {
    int a,b,c;
    scanf("%d",&a);
    scanf("%d",&b);
    scanf("%d",&c); 

    if(a>b && b>c){
        printf("The largest number is : %d",a);
    }
    else if(a>=c && c>=b){
        printf("The largest number is : %d",a);
    }
    else if(b>=a && a>=c){
        printf("The largest number is : %d",b);
    }
    else if(b>=c && c>=a){
        printf("The largest number is : %d",b);
    }
   
    else if(c>=a && a>=b){
        printf("The largest number is : %d",c);
    }
    else if(c>=b && b>=a){
        printf("The largest number is : %d",c);
    }
    return 0;
}
